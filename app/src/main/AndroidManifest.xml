<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mis.smakash.fotonapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        tools:replace="android:appComponentFactory"
        android:appComponentFactory="whateverString"
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@drawable/ic_menu"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_menu"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PeriodicalEntryType"></activity>
        <activity android:name=".ServiceSatisfaction" />
        <activity android:name=".GoodWillEntry" />
        <activity android:name=".SixHourActivity" />
        <activity android:name=".CustomerSatisfaction" />
        <activity android:name=".ServiceRatio" />
        <activity android:name=".ServicePerformance" />
        <activity android:name=".ViewAllServices" />
        <activity
            android:name=".PostwarrentyEntry"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PaidEntry"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WarrentyEntry"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PeriodicalEntry"
            android:screenOrientation="portrait" />
        <activity
            android:name=".InstallationEntry"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ServiceType"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ServiceCall"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelectProduct"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>